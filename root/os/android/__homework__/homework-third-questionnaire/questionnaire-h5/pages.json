{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/fill/fill",
      "style": {
        "navigationBarTitleText": "填写问卷",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/create/create",
      "style": {
        "navigationBarTitleText": "创建问卷",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": true
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "问卷系统",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },

  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      //
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/home.png",
        "selectedIconPath": "static/home.png"
      },
      {
        "pagePath": "pages/fill/fill",
        "text": "填写",
        "iconPath": "static/fill.png",
        "selectedIconPath": "static/fill.png"
      },
      {
        "pagePath": "pages/create/create",
        "text": "创建",
        "iconPath": "static/create.png",
        "selectedIconPath": "static/create.png"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "static/user.png",
        "selectedIconPath": "static/user.png"
      }
    ]
  }
}
